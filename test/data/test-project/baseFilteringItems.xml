<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<filteringItems>
    <filteringItem>
        <apply>true</apply>
        <pattern>^(?:(\\d+(?:[\\.,]\\d+)?)(?:\\|)?)+</pattern>
        <example>30,21</example>
    </filteringItem>
    <filteringItem>
        <apply>true</apply>
        <pattern>(0[1-9]|[12][0-9]|3[01])[- /.]([1-9]|[1-9]|0[1-9]|1[012])[- /.](19|20)\\d\\d</pattern>
        <example>12.01.2010</example>
    </filteringItem>
    <filteringItem>
        <apply>true</apply>
        <pattern>^[-\\w.]+@([A-z0-9][-A-z0-9]+\\.)+[A-z]{2,4}$</pattern>
        <example>sieger_116@mail.ru</example>
    </filteringItem>
